{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Youtube video",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "readonly": true,
      "default": "youtube",
      "options": {
        "hidden": true
      }
    },
    "src": {
      "type": "string",
      "description": "The youtube embed url. Example: https://www.youtube.com/embed/8X1eao17Lok"
    }
  },
  "required": ["src"]
}
