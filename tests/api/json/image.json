{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Image",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "readonly": true,
      "default": "image",
      "options": {
        "hidden": true
      }
    },
    "src": {
      "type": "string",
      "format": "url"
    },
    "alt": {
      "type": "string"
    }
  },
  "required": ["src"]
}
